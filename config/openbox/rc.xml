    </keybind>
    
    <keybind key="W-n">
    <action name="Execute">
      <command>rofi-wifi</command>
    </action>
  </keybind>
  
  <keybind key="W-S-e">
    <action name="Execute">
      <command>rofi-emoji</command>
    </action>
  </keybind>
